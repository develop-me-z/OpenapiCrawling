{{>layout/header}}

<div class="container">
    <div class="sub-title">
        <h4>양수장시설제원</h4>
    </div>

    <div class="textAlignCenter searchBox">
        <label for="admcd"> 행정구역 : </label>
        <select id="admcd">
            <option value="">전체</option>
            <option value="11">서울특별시</option>
            <option value="26">부산광역시</option>
            <option value="27">대구광역시</option>
            <option value="28">인천광역시</option>
            <option value="29">광주광역시</option>
            <option value="30">대전광역시</option>
            <option value="31">울산광역시</option>
            <option value="41">경기도</option>
            <option value="42">강원도</option>
            <option value="43">충청북도</option>
            <option value="44">충청남도</option>
            <option value="45">전라북도</option>
            <option value="46">전라남도</option>
            <option value="47">경상북도</option>
            <option value="48">경상남도</option>
            <option value="50">제주특별자치도</option>
        </select>

        <label for="basin">권역 :</label>
        <select id="basin">
            <option value="">전체</option>
            <option value="1">한강</option>
            <option value="2">낙동강</option>
            <option value="3">금강</option>
            <option value="4">섬진강</option>
            <option value="5">영산강</option>
            <option value="6">제주도</option>
        </select>

        <input id="btnSearchW21" type="button" class="btn btn-outline-info" style="margin-left:10px;" value="조회"/>
    </div>
    <div class="saveBox">
        <a id="btnSave" href="" class="btn btn-outline-success" style="margin-left:10px;" value="csv로 저장">csv로 저장</a>
    </div>
    <!--목록 출력 영역-->
    <table id="dataTable" class="table table-horizontal table-bordered">
        <thead class="thead-strong">
        <tr>
            <th>시도명</th>
            <th>시도코드</th>
            <th>시설물수(개소)</th>
            <th>유역면적(ha)</th>
            <th>원동기대수(대)</th>
            <th>최대양수량(CMS)</th>
            <th>인가면적(ha)</th>
            <th>관개면적(ha)</th>
            <th>순관개면적(ha)</th>
            <th>관배수면적(ha)</th>
            <th>구역외급수면적(ha)</th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#data}}
            <tr>
                <td>{{admnm}}</td>
                <td>{{admcd}}</td>
                <td>{{estcnt}}</td>
                <td>{{bsnara}}</td>
                <td>{{mtcnt}}</td>
                <td>{{mxvol}}</td>
                <td>{{pemara}}</td>
                <td>{{totirara}}</td>
                <td>{{nirara}}</td>
                <td>{{pirara}}</td>
                <td>{{oirara}}</td>
            </tr>
        {{/data}}
        {{^data}}
            <tr>
                <td colspan="11">해당 데이터가 없습니다.</td>
            </tr>
        {{/data}}
        </tbody>
    </table>
    <div style="height: 100px;"></div>
</div>


{{>layout/footer}}