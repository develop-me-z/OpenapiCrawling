{{>layout/header}}

<div class="container">
    <div class="sub-title">
        <h4>강수량 관측소 제원</h4>
    </div>

    <div class="textAlignCenter searchBox">
        <label for="obscd">관측소코드(필수) : </label>
        <input id="obscd" type="text" style="margin-left:10px;"/>

        <input id="btnSearchW2" type="button" class="btn btn-outline-info" style="margin-left:10px;" value="조회"/>
    </div>
    <div class="saveBox">
        <a id="btnSave" href="" class="btn btn-outline-success" style="margin-left:10px;" value="csv로 저장">csv로 저장</a>
    </div>
    <!--목록 출력 영역-->
    <table id="dataTable" class="table table-horizontal table-bordered">
        <thead class="thead-strong"></thead>
        <tbody id="tbody">
        {{#data}}
            <tr>
                <th>관측소명(한글)</th>
                <td>{{obsnm}}</td>
            </tr>
            <tr>
                <th>관측소명(영문)</th>
                <td>{{obsnmeng}}</td>
            </tr>
            <tr>
                <th>관측소코드</th>
                <td>{{obscd}}</td>
            </tr>
            <tr>
                <th>관할기관</th>
                <td>{{mngorg}}</td>
            </tr>
            <tr>
                <th>수계</th>
                <td>{{bbsnnm}}</td>
            </tr>
            <tr>
                <th>표준유역코드</th>
                <td>{{sbsncd}}</td>
            </tr>
            <tr>
                <th>관측개시일</th>
                <td>{{opendt}}</td>
            </tr>
            <tr>
                <th>관측기종</th>
                <td>{{obsknd}}</td>
            </tr>
            <tr>
                <th>주소(지번주소)</th>
                <td>{{addr}}</td>
            </tr>
            <tr>
                <th>경도(GPS좌표계)</th>
                <td>{{lon}}</td>
            </tr>
            <tr>
                <th>위도(GPS좌표계)</th>
                <td>{{lat}}</td>
            </tr>
            <tr>
                <th>해발고(EL.m)</th>
                <td>{{shgt}}</td>
            </tr>
            <tr>
                <th>자료보유기간-시자료-시작일</th>
                <td>{{hrdtstart}}</td>
            </tr>
            <tr>
                <th>자료보유기간-시자료-최종일</th>
                <td>{{hrdtend}}</td>
            </tr>
            <tr>
                <th>자료보유기간-일자료-시작일</th>
                <td>{{dydtstart}}</td>
            </tr>
            <tr>
                <th>자료보유기간-일자료-종료일</th>
                <td>{{dydtend}}</td>
            </tr>
        {{/data}}
        </tbody>
    </table>
    <div style="height: 100px;"></div>
</div>


{{>layout/footer}}