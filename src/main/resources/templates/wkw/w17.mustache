{{>layout/header}}

<div class="container">
    <div class="sub-title">
        <h4>유량 측정성과</h4>
    </div>

    <div class="textAlignCenter searchBox">
        <label for="obscd">관측소코드(필수) : </label>
        <input id="obscd" type="text" style="margin-left:10px;"/>

        <label for="startyear">관측기간(시작년도:YYYY): </label>
        <input id="startyear" type="text" style="margin-left:10px;"/>
        ~
        <label for="endyear">관측기간(종료년도:YYYY): </label>
        <input id="endyear" type="text" style="margin-left:10px;"/>

        <input id="btnSearchW17" type="button" class="btn btn-outline-info" style="margin-left:10px;" value="조회"/>
    </div>
    <div class="saveBox">
        <a id="btnSave" href="" class="btn btn-outline-success" style="margin-left:10px;" value="csv로 저장">csv로 저장</a>
    </div>
    <!--목록 출력 영역-->
    <table id="dataTable" class="table table-horizontal table-bordered">
        <thead class="thead-strong">
        <tr>
            <th>측정년월일</th>
            <th>시작시 분</th>
            <th>종료시 분</th>
            <th>수위 시작</th>
            <th>수위 종료</th>
            <th>수위 평균</th>
            <th>하폭</th>
            <th>단면적</th>
            <th>유속</th>
            <th>유량</th>
            <th>측정방법</th>
            <th>인용문헌</th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#data}}
            <tr>
                <td>{{obsymd}}</td>
                <td>{{obssthm}}</td>
                <td>{{obsedhm}}</td>
                <td>{{stwl}}</td>
                <td>{{edwl}}</td>
                <td>{{avgwl}}</td>
                <td>{{rivwith}}</td>
                <td>{{care}}</td>
                <td>{{wspd}}</td>
                <td>{{flw}}</td>
                <td>{{obsway}}</td>
                <td>{{docnm}}</td>
            </tr>
        {{/data}}
        {{^data}}
            <tr>
                <td colspan="12">해당 데이터가 없습니다.</td>
            </tr>
        {{/data}}
        </tbody>
    </table>
    <div style="height: 100px;"></div>
</div>


{{>layout/footer}}