{{>layout/header}}

<div class="container">
    <div class="sub-title">
        <h4>유량 측정성과 관측소 검색</h4>
    </div>

    <div class="textAlignCenter searchBox">
        <label for="basin"> 권역 : </label>
        <select id="basin">
            <option value="">전체</option>
            <option value="1">한강</option>
            <option value="2">낙동강</option>
            <option value="3">금강</option>
            <option value="4">섬진강</option>
            <option value="5">영산강</option>
            <option value="6">제주도</option>
        </select>

        <label for="mngorg">관할기관 :</label>
        <select id="mngorg">
            <option value="">전체</option>
            <option value="1">환경부</option>
            <option value="2">한국수자원공사</option>
        </select>

        <input id="btnSearchW16" type="button" class="btn btn-outline-info" style="margin-left:10px;" value="조회"/>
    </div>
    <div class="saveBox">
        <a id="btnSave" href="" class="btn btn-outline-success" style="margin-left:10px;" value="csv로 저장">csv로 저장</a>
    </div>
    <!--목록 출력 영역-->
    <table id="dataTable" class="table table-horizontal table-bordered">
        <thead class="thead-strong">
        <tr>
            <th>관측소코드</th>
            <th>관측소명</th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#data}}
            <tr>
                <td>{{wlobscd}}</td>
                <td>{{obsnm}}</td>
            </tr>
        {{/data}}
        {{^data}}
            <tr>해당 데이터가 없습니다.</tr>
        {{/data}}
        </tbody>
    </table>
    <div style="height: 100px;"></div>
</div>


{{>layout/footer}}