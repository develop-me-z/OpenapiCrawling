{{>layout/header}}

<div class="container">
    <div class="sub-title">
        <h4>강수량 일자료</h4>
    </div>

    <div class="textAlignCenter searchBox">

        <label for="obscd">관측소코드(필수) : </label>
        <input id="obscd" type="text" style="margin-left:10px;"/>

        <label for="startyear">관측기간(시작년도-YYYY): </label>
        <input id="startyear" type="text" style="margin-left:10px;"/>
        ~
        <label for="endyear">관측기간(종료년도-YYYY): </label>
        <input id="endyear" type="text" style="margin-left:10px;"/>

        <input id="btnSearchW5" type="button" class="btn btn-outline-info" style="margin-left:10px;" value="조회"/>
    </div>
    <div class="saveBox">
        *2014년 이후의 자료가 조회됩니다.*
        <a id="btnSave" href="" class="btn btn-outline-success" style="margin-left:10px;" value="csv로 저장">csv로 저장</a>
    </div>
    <!--목록 출력 영역-->
    <table id="dataTable" class="table table-horizontal table-bordered">
        <thead class="thead-strong">
        <tr>
            <th>관측월</th>
            <th>강수량(mm)</th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#data}}
            <tr>
                <td>{{ym}}</td>
                <td>{{dtrf}}</td>
            </tr>
        {{/data}}
        </tbody>
    </table>
    <div style="height: 100px;"></div>
</div>


{{>layout/footer}}